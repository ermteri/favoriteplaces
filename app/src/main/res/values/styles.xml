<resources>
    <!-- Base application theme. -->
    <!--style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar"-->
    <style name="AppTheme" parent="android:Theme.Holo">
        <!-- Customize your theme here. -->
    </style>
    <style name="Headings"
        parent="@android:style/TextAppearance.Medium">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#00FF00</item>
        <item name="android:typeface">normal</item>
    </style>

    <style name="Theme.Custom" parent="android:Theme.Holo">
        <item name="android:calendarViewStyle">@style/Widget.CalendarView.Custom</item>
    </style>

    <style name="Widget.CalendarView.Custom" parent="android:Widget.CalendarView">
        <!--item name="android:focusedMonthDateColor">@color/cs_textcolor</item>
        <item name="android:weekNumberColor">@color/red</item-->
        <item name="android:weekDayTextAppearance">@style/TextAppearance.AppCompat.Large</item>
        <item name="android:dateTextAppearance">@style/TextAppearance.AppCompat.Medium</item>
    </style>

</resources>